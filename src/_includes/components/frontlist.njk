<div class="list">
  <h1>All blog posts</h1>
  <ul class="bloglist">
    {% for post in collections.post %}
      <li>
        <a href="{{ post.url }}">{{ post.data.title }}</a>
        {% if post.data.excerpt %}
          <p>{{ post.data.excerpt }}</p>
        {% endif %}
      </li>
    {% endfor %}
  </ul>
</div>